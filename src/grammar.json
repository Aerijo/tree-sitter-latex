{
  "name": "latex",
  "rules": {
    "tex": {
      "type": "SYMBOL",
      "name": "text_mode"
    },
    "text_mode": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "active_char"
          },
          {
            "type": "SYMBOL",
            "name": "command"
          },
          {
            "type": "SYMBOL",
            "name": "escaped"
          },
          {
            "type": "SYMBOL",
            "name": "parameter"
          },
          {
            "type": "SYMBOL",
            "name": "subscript"
          },
          {
            "type": "SYMBOL",
            "name": "superscript"
          },
          {
            "type": "SYMBOL",
            "name": "text"
          },
          {
            "type": "SYMBOL",
            "name": "inline_math"
          },
          {
            "type": "SYMBOL",
            "name": "display_math"
          },
          {
            "type": "SYMBOL",
            "name": "text_group"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          }
        ]
      }
    },
    "math_mode": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "active_char"
          },
          {
            "type": "SYMBOL",
            "name": "command"
          },
          {
            "type": "SYMBOL",
            "name": "escaped"
          },
          {
            "type": "SYMBOL",
            "name": "parameter"
          },
          {
            "type": "SYMBOL",
            "name": "subscript"
          },
          {
            "type": "SYMBOL",
            "name": "superscript"
          },
          {
            "type": "SYMBOL",
            "name": "text"
          },
          {
            "type": "SYMBOL",
            "name": "math_group"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          }
        ]
      }
    },
    "parameter": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "parameter_char"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "escaped": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "escape"
        },
        {
          "type": "PATTERN",
          "value": "."
        }
      ]
    },
    "command": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "escape"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "storage"
            },
            {
              "type": "SYMBOL",
              "name": "catcode"
            },
            {
              "type": "SYMBOL",
              "name": "keyword"
            }
          ]
        }
      ]
    },
    "storage": {
      "type": "PATTERN",
      "value": "[egx]?def"
    },
    "catcode": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "k?catcode`"
        },
        {
          "type": "SYMBOL",
          "name": "escaped"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "text_group": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "begin_group"
        },
        {
          "type": "SYMBOL",
          "name": "text_mode"
        },
        {
          "type": "SYMBOL",
          "name": "end_group"
        }
      ]
    },
    "math_group": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "begin_group"
        },
        {
          "type": "SYMBOL",
          "name": "math_mode"
        },
        {
          "type": "SYMBOL",
          "name": "end_group"
        }
      ]
    },
    "display_math": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "math_shift"
        },
        {
          "type": "SYMBOL",
          "name": "math_shift"
        },
        {
          "type": "SYMBOL",
          "name": "math_mode"
        },
        {
          "type": "SYMBOL",
          "name": "math_shift"
        },
        {
          "type": "SYMBOL",
          "name": "math_shift"
        }
      ]
    },
    "inline_math": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "math_shift"
        },
        {
          "type": "SYMBOL",
          "name": "math_mode"
        },
        {
          "type": "SYMBOL",
          "name": "math_shift"
        }
      ]
    },
    "escape": {
      "type": "STRING",
      "value": "\\"
    },
    "begin_group": {
      "type": "STRING",
      "value": "{"
    },
    "end_group": {
      "type": "STRING",
      "value": "}"
    },
    "math_shift": {
      "type": "STRING",
      "value": "$"
    },
    "alignment_tab": {
      "type": "STRING",
      "value": "&"
    },
    "end_of_line": {
      "type": "STRING",
      "value": "\n"
    },
    "parameter_char": {
      "type": "STRING",
      "value": "#"
    },
    "superscript": {
      "type": "STRING",
      "value": "^"
    },
    "subscript": {
      "type": "STRING",
      "value": "_"
    },
    "space": {
      "type": "PATTERN",
      "value": "[ \\t]"
    },
    "letter": {
      "type": "PATTERN",
      "value": "[a-zA-Z]"
    },
    "keyword": {
      "type": "PATTERN",
      "value": "[a-zA-Z]+"
    },
    "other": {
      "type": "PATTERN",
      "value": "[^\\\\{}$&\\n#^_ \\ta-zA-Z~%]"
    },
    "active_char": {
      "type": "STRING",
      "value": "~"
    },
    "comment_char": {
      "type": "STRING",
      "value": "%"
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\\\{}$&#^_~%]+"
    },
    "number": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "comment": {
      "type": "PATTERN",
      "value": "%.*"
    },
    "magic": {
      "type": "PATTERN",
      "value": "%\\s+!T[eE]X\\s+(\\w+:)?\\w+\\s*=.*"
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "magic"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": []
}